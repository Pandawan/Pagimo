{
	"name": "pagimo",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "nodemon ./bin/www"
	},
	"dependencies": {
		"body-parser": "~1.18.2",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"ejs": "~2.5.7",
		"express": "~4.15.5",
		"morgan": "~1.9.0",
		"serve-favicon": "~2.4.5"
	},
	"devDependencies": {
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^12.1.0",
		"eslint-plugin-import": "^2.9.0"
	},
	"eslintConfig": {
		"extends": "airbnb-base",
		"env": {
			"es6": true,
			"browser": true
		},
		"rules": {
			"brace-style": [
				"error",
				"stroustrup"
			],
			"comma-dangle": [
				"error",
				"never"
			],
			"no-unused-vars": [
				"warn"
			],
			"one-var": [
				"off"
			]
		}
	}
}